import java.util.Scanner;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n= sc.nextInt();
        if(n<0){
            n = -n;}
        int ld = n % 10;
        int fd = n;
        while (fd >= 10) {
            fd /= 10;
        }

        int sum = fd + ld;
        System.out.println(sum);
    }
}
