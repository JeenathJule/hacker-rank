import java.util.Scanner;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();

        if (n == 0) {
            System.out.println(1); 
        } else {
            if (n < 0) {
                n = -n; 
            }
            int count = 0;
            while (n > 0) {
                count++;
                n /= 10;
            }
            System.out.println(count);
        }

        sc.close();
    }
}
