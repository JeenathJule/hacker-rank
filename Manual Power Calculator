import java.util.Scanner;
public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int b = sc.nextInt();
        int e= sc.nextInt();
        int r = 1;
        for (int i = 1; i <= e; i++) {
            r = r * b;
        }
        System.out.println(r);
    }
}
