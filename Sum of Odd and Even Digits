 import java.util.Scanner;

public class CountDigits {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n= sc.nextInt();
        int even=0,odd=0;
        while(n!=0){
            if(n<0)
                n=-n;
            int m=n%10;
            if(m%2==0)
                even+=m;
            else
                odd+=m;
            n=n/10;
        }
        System.out.println(odd+" "+even);
    }
}
